{
  "name": "@subsquid/cli",
  "description": "squid cli tool",
  "version": "1.0.0",
  "license": "GPL-3.0-or-later",
  "repository": "git@github.com:subsquid/squid.git",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "sqd": "./bin/run.js"
  },
  "files": [
    "bin",
    "lib",
    "src",
    "src/marshal.ts",
    "oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sqd",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "db": {
        "description": "Database management commands"
      },
      "squid": {
        "description": "Squid management commands (SaaS)"
      },
      "archive": {
        "description": "Squid archive commands (SaaS)"
      }
    }
  },
  "homepage": "https://www.subsquid.io/",
  "scripts": {
    "build": "rm -rf lib && tsc"
  },
  "dependencies": {
    "@oclif/core": "^1.3.0",
    "@oclif/plugin-help": "^5.1.11",
    "@octokit/auth-oauth-device": "^3.1.2",
    "@subsquid/openreader": "^1.0.0",
    "@subsquid/typeorm-config": "^1.0.0",
    "@subsquid/util-internal": "^0.0.0",
    "@subsquid/util-internal-code-printer": "^0.0.0",
    "@subsquid/util-naming": "^0.0.0",
    "chalk": "^4.1.2",
    "cli-select": "^1.1.2",
    "dotenv": "^10.0.0",
    "oclif": "^2.4.3",
    "node-fetch": "^2.6.7",
    "pg": "^8.7.3",
    "pgtools": "^0.3.2",
    "query-string": "^7.1.0",
    "simple-git": "^2.48.0",
    "typeorm": "^0.2.41",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@subsquid/substrate-processor": "^1.0.0",
    "@types/node": "^16.11.22",
    "@types/node-fetch": "^2.5.12",
    "@types/js-yaml": "^4.0.5",
    "typescript": "~4.5.5"
  }
}
