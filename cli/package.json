{
  "name": "@subsquid/cli",
  "description": "squid cli tool",
  "version": "1.2.0",
  "license": "GPL-3.0-or-later",
  "repository": "git@github.com:subsquid/squid.git",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "sqd": "./bin/run"
  },
  "files": [
    "bin",
    "lib",
    "src",
    "oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sqd",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "squid": {
        "description": "Squid management commands (SaaS)"
      }
    }
  },
  "homepage": "https://www.subsquid.io/",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "dev": "./bin/dev"
  },
  "dependencies": {
    "@oclif/core": "^1.9.5",
    "@oclif/plugin-help": "^5.1.12",
    "@octokit/auth-oauth-device": "^3.1.2",
    "chalk": "^4.1.2",
    "cli-select": "^1.1.2",
    "cli-table3": "^0.6.2",
    "inquirer": "^8.2.4",
    "oclif": "^2.6.3",
    "node-fetch": "^2.6.7",
    "query-string": "^7.1.1",
    "simple-git": "^2.48.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.41",
    "@types/inquirer": "^8.2.1",
    "@types/node-fetch": "^2.6.2",
    "@types/js-yaml": "^4.0.5",
    "typescript": "~4.7.4"
  }
}